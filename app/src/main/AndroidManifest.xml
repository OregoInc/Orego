<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.orego.corporation.orego">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app.name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity
            android:name=".fragments.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fragments.otherActivities.camera.CameraActivity"
            android:parentActivityName=".fragments.otherActivities.OldMainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragments.otherActivities.face3dActivity.model3D.view.DemoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragments.otherActivities.face3dActivity.model3D.view.ModelActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragments.otherActivities.OldMainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>